{
  "private": true,
  "name": "browser-app",
  "version": "0.1.0",
  "dependencies": {
    "@glsp-examples/workflow-theia": "0.1.0",
    "@theia/core": "^0.5.0",
    "@theia/editor": "^0.5.0",
    "@theia/filesystem": "^0.5.0",
    "@theia/languages": "^0.5.0",
    "@theia/markers": "^0.5.0",
    "@theia/messages": "^0.5.0",
    "@theia/monaco": "^0.5.0",
    "@theia/navigator": "^0.5.0",
    "@theia/preferences": "^0.5.0",
    "@theia/process": "^0.5.0",
    "@theia/terminal": "^0.5.0",
    "@theia/typescript": "^0.5.0",
    "@theia/workspace": "^0.5.0"
  },
  "devDependencies": {
    "@theia/cli": "^0.5.0"
  },
  "scripts": {
    "prepare": "theia build --mode development",
    "start": "theia start --WORKFLOW_LSP=5007 --root-dir=../workspace",
    "start:debug": "theia start --WORKFLOW_LSP=5007 --root-dir=../workspace --loglevel=debug",
    "watch": "theia build --watch --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
